{
  "version": "1.0.0",
  "description": "Aurora Token Catalog - Machine-readable token definitions",
  "keywords": [
    { "text": "if", "kind": "if" },
    { "text": "else", "kind": "else" },
    { "text": "match", "kind": "match" },
    { "text": "for", "kind": "for" },
    { "text": "while", "kind": "while" },
    { "text": "loop", "kind": "loop" },
    { "text": "break", "kind": "break" },
    { "text": "continue", "kind": "continue" },
    { "text": "return", "kind": "return" },
    { "text": "yield", "kind": "yield" },
    { "text": "fn", "kind": "fn" },
    { "text": "let", "kind": "let" },
    { "text": "mut", "kind": "mut" },
    { "text": "const", "kind": "const" },
    { "text": "static", "kind": "static" },
    { "text": "type", "kind": "type" },
    { "text": "trait", "kind": "trait" },
    { "text": "impl", "kind": "impl" },
    { "text": "use", "kind": "use" },
    { "text": "mod", "kind": "mod" },
    { "text": "pub", "kind": "pub" },
    { "text": "as", "kind": "as" },
    { "text": "self", "kind": "self_lower" },
    { "text": "Self", "kind": "self_upper" },
    { "text": "super", "kind": "super" },
    { "text": "crate", "kind": "crate" },
    { "text": "async", "kind": "async" },
    { "text": "await", "kind": "await" },
    { "text": "defer", "kind": "defer" },
    { "text": "unsafe", "kind": "unsafe" },
    { "text": "comptime", "kind": "comptime" },
    { "text": "true", "kind": "true" },
    { "text": "false", "kind": "false" },
    { "text": "Some", "kind": "some" },
    { "text": "None", "kind": "none" },
    { "text": "Ok", "kind": "ok" },
    { "text": "Err", "kind": "err" },
    { "text": "unreachable", "kind": "unreachable" }
  ],
  "operators": {
    "arithmetic": ["+", "-", "*", "/", "%", "**"],
    "comparison": ["==", "!=", "<", "<=", ">", ">="],
    "logical": ["&&", "||", "!"],
    "bitwise": ["&", "|", "^", "<<", ">>", "~"],
    "assignment": ["=", "+=", "-=", "*=", "/=", "%=", "&=", "|=", "^=", "<<=", ">>="],
    "access": [".", "..", "..=", "...", "::", "->", "=>", "?", "??"],
    "pipeline": ["|>", "<|"]
  },
  "delimiters": ["(", ")", "{", "}", "[", "]", ",", ";", ":"],
  "disambiguation_rules": {
    "maximal_munch": "Longest match wins (e.g., '..' vs '.')",
    "reserved_word_priority": "Keywords take precedence over identifiers",
    "lookahead": "Used for '.', '..', '..=', '...' disambiguation"
  }
}
